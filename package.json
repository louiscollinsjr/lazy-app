		"@fontsource/playfair-display": "^5.2.5",
		"@fontsource/roboto": "^5.2.5",
		"@fontsource/source-serif-pro": "^5.2.5",
		"@fontsource/tiro-bangla": "^5.2.5",
		"@inlang/paraglide-js": "^2.0.0",
		"@supabase/supabase-js": "^2.49.4",
		"fontsource-inter": "^4.0.0",
		"tinacms": "^2.7.7"
	},
	"tina": {
		"config": "tina/config.ts"
	}
